{
  "name": "@wingriders/governance-sdk",
  "version": "1.0.2",
  "description": "A library to interact with the WingRiders On-Chain DAO governance",
  "repository": {
    "type": "git",
    "url": "https://github.com/WingRiders/on-chain-dao-governance.git"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/node/index.cjs",
      "import": "./dist/es/index.js",
      "default": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@wingriders/cab": "^2.1.7",
    "buffer": "^6.0.3",
    "date-fns": "^3.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.3",
    "@vitest/coverage-v8": "^1.2.2",
    "msw": "^2.2.2",
    "rollup": "^4.6.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.2",
    "vitest": "^1.2.2"
  }
}
